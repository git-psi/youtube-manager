{
  "name": "gestionnaire-de-musique",
  "version": "1.0.0",
  "repository": "https://github.com/git-psi/youtube-manager",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.youtube-manager.id",
    "productName": "Gestionnaire de Musique",
    "executableName": "Gestionnaire-de-Musique",
    "artifactName": "Gestionnaire-de-Musique-Setup-${version}.${ext}",
    "icon": "build/icon.ico",
    "win": {
      "icon": "build/icon.ico",
      "target": ["nsis"]
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "linux": {
      "icon": "build/icon.png",
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Audio"
    }
  },
  "main": "backend/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:linux": "electron-builder --linux"
  },
  "keywords": [],
  "author": "FÃ©lix Perdu",
  "license": "ISC",
  "description": "description",
  "devDependencies": {
    "@electron/packager": "^18.3.4",
    "electron": "^32.0.1",
    "electron-builder": "^25.1.7"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.15.1",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "dotenv": "^16.4.5",
    "electron-store": "^7.0.3",
    "electron-updater": "^6.3.9",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "npm": "^10.9.0",
    "youtubei.js": "^10.5.0"
  }
}
